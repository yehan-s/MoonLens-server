# 在你的 .env 文件中添加以下配置

# GitLab OAuth
GITLAB_CLIENT_ID=your_gitlab_app_id_here
GITLAB_CLIENT_SECRET=your_gitlab_app_secret_here
GITLAB_REDIRECT_URI=http://localhost:3000/auth/gitlab/callback

# GitHub OAuth
GITHUB_CLIENT_ID=your_github_app_id_here
GITHUB_CLIENT_SECRET=your_github_app_secret_here
GITHUB_REDIRECT_URI=http://localhost:3000/auth/github/callback

# Frontend URL
FRONTEND_URL=http://localhost:5173

# JWT (已有的保持不变)
JWT_SECRET=dev-super-secret
JWT_EXPIRES_IN=7d
