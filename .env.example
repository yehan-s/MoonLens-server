# 应用配置
NODE_ENV=development
PORT=3000

# 数据库配置（支持 mysql 或 postgres）
DB_TYPE=mysql
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=moonlens
DB_PASSWORD=moonlens_password
DB_DATABASE=moonlens_db

# JWT 配置
JWT_SECRET=your-secret-key-here
JWT_EXPIRES_IN=7d

# GitLab 配置
# GitLab API 基础地址（云： https://gitlab.com，自建：例如 https://gitlab.example.com）
GITLAB_BASE_URL=https://gitlab.com
# GitLab Webhook 验证密钥（用于接收仓库事件，可选）
GITLAB_WEBHOOK_TOKEN=your-webhook-secret-token
GITLAB_WEBHOOK_SECRET=your-webhook-secret
# GitLab Bot Token（用于发布 AI 审查评论到 MR，需要 api 权限）
GITLAB_BOT_TOKEN=your-gitlab-bot-token-here

# GitLab OAuth（用于用户使用 GitLab 账号登录）
# 在 GitLab Applications 中创建 OAuth 应用后填入以下三项
GITLAB_OAUTH_CLIENT_ID=
GITLAB_OAUTH_CLIENT_SECRET=
# 回调地址需要与 GitLab 应用保持一致，保持与后端路由 `/api/auth/gitlab/callback` 对齐
GITLAB_OAUTH_CALLBACK_URL=http://localhost:3000/api/auth/gitlab/callback

# GitHub 配置
# GitHub Personal Access Token (用于访问 GitHub API)
GITHUB_TOKEN=your-github-token-here
# GitHub Bot Token (用于发布 AI 审查评论到 PR，需要 repo 权限)
GITHUB_BOT_TOKEN=your-github-bot-token-here
# GitHub Webhook Secret (用于验证 webhook 请求)
GITHUB_WEBHOOK_SECRET=your-github-webhook-secret

# GitHub OAuth（用于用户使用 GitHub 账号登录）
GITHUB_OAUTH_CLIENT_ID=
GITHUB_OAUTH_CLIENT_SECRET=
GITHUB_OAUTH_CALLBACK_URL=http://localhost:3000/api/auth/github/callback

# AI 服务配置
AI_SERVICE_URL=http://localhost:8000
AI_API_KEY=your-ai-api-key
AI_MODEL=gpt-4

# Redis 配置（用于缓存和队列）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# WebSocket 配置
WS_PORT=3001

# 日志级别
LOG_LEVEL=info
